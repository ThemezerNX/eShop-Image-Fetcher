{
    "name": "eShop-Image-Scraper",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "https://github.com/ThemezerNX/eShop-Image-Scraper.git",
    "author": "Migushthe2nd",
    "license": "MIT",
    "scripts": {
        "start": "node ./index.test.js",
        "dev": "nodemon",
        "build": "tsc ./index.ts ./index.test.ts",
        "postinstall": "tsc ./index.ts"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "exec": "yarn run build && yarn run start",
        "ext": "ts"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "axios-cookiejar-support": "^1.0.1",
        "cheerio": "^1.0.0-rc.3",
        "cookiejar": "^2.1.2",
        "dotenv": "^8.2.0",
        "tough-cookie": "^4.0.0",
        "typescript": "^4.1.3"
    }
}
